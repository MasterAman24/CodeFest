class Sum_Of_Digits
{
    public static void main(String []args)
    {
        long n = (long)Math.pow(2,1450);
        long N = n;
        long sum=0;
        long d;
        long count=0;
        while(n>0)
        {
            N/=10;
            count++;          
        }
        System.out.println(count);
        while(n>0)
        {
            d=n%10;
            n=n/10;
            sum+=d;
        }
        System.out.println("The sum of digits = "+sum);    
    }
}
